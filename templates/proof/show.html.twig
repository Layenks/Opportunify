{% extends 'base.html.twig' %}

{% block title %}Proof{% endblock %}

{% block body %}
    <h1>Proof</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proof.id }}</td>
            </tr>
            <tr>
                <th>Images</th>
                <td>{{ proof.images ? proof.images|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>DateUpload</th>
                <td>{{ proof.dateUpload ? proof.dateUpload|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_proof_index') }}">back to list</a>

    <a href="{{ path('app_proof_edit', {'id': proof.id}) }}">edit</a>

    {{ include('proof/_delete_form.html.twig') }}
{% endblock %}
